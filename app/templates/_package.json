{
  "name": "<%= moduleName %>",
  "main": "./src",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "standard && babel-tape-runner test/*.js"
  },
  "keywords": [
    <%= keywordString %>
  ],
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.0.0",
    "babel-tape-runner": "*",
    "tape": "^4.0.0",
    "standard": "bendrucker/standard#babel",
    "publicist": "~4.0.0",
    "publicist-es5": "~1.0.0"
  },
  "publicist": {
    "builds": {
      "es5": {
        "dest": "./release"
      }
    }
  },
  "standard": {
    "ignore": [
      "release/",
      "lib/"
    ]
  }
}
